<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="16.0dip"
    android:layout_marginTop="16.0dip"
    android:layout_marginRight="16.0dip"
    android:layout_marginBottom="10.0dip"
    app:cardBackgroundColor="@android:color/transparent"
    app:cardCornerRadius="28.0dip"
    app:cardElevation="0.0dip"
    app:strokeWidth="0.0dip"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <com.google.android.material.card.MaterialCardView
            android:clickable="false"
            android:clipToPadding="false"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0.0dip"
            android:layout_marginTop="0.0dip"
            android:layout_marginRight="0.0dip"
            android:layout_marginBottom="0.0dip"
            app:cardBackgroundColor="?colorCard"
            app:cardCornerRadius="5.0dip"
            app:cardElevation="0.0dip"
            app:rippleColor="@android:color/transparent">
            <LinearLayout
                android:gravity="center_vertical"
                android:padding="16.0dip"
                android:clipToPadding="false"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false">
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="150.0dip"
                    android:layout_height="fill_parent"
                    app:cardBackgroundColor="#00000000"
                    app:cardCornerRadius="18.0dip"
                    app:cardElevation="0.0dip">
                    <com.flaviofaria.kenburnsview.KenBurnsView
                        android:id="@+id/img_banner_preference"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:src="@drawable/uwu_banner_image"
                        android:adjustViewBounds="true" />
                    <LinearLayout
                        android:background="@android:color/black"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:alpha="0.2" />
                    <LinearLayout
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                        <TextClock
                            android:textSize="56.0sp"
                            android:typeface="monospace"
                            android:textColor="@android:color/white"
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="-12.0dip"
                            android:fontFamily="sans-serif"
                            android:format12Hour="hh"
                            android:format24Hour="kk"
                            android:fontFeatureSettings="tnum, cv03, cv04" />
                        <TextClock
                            android:textSize="56.0sp"
                            android:typeface="monospace"
                            android:textColor="@android:color/white"
                            android:gravity="center"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="-12.0dip"
                            android:fontFamily="sans-serif"
                            android:format12Hour="mm"
                            android:format24Hour="mm"
                            android:fontFeatureSettings="tnum, cv03, cv04" />
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>
                <RelativeLayout
                    android:padding="16.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0">
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_name_title"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_name_title"
                        android:singleLine="true" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_name_title_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12.0dip"
                        android:text="@string/app_name"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_name_title"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_name_title" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_mikubox_author"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_mikubox_author_title"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_name_title_summary"
                        android:layout_alignStart="@+id/uwu_name_title_summary" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_mikubox_author_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12.0dip"
                        android:text="HatsuneMikuUwU"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_mikubox_author"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_mikubox_author" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_build_date"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_build_date_title"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_mikubox_author_summary"
                        android:layout_alignStart="@+id/uwu_mikubox_author_summary" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_build_date_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12.0dip"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_build_date"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_build_date" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_version_name"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_version_name_title"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_build_date_summary"
                        android:layout_alignStart="@+id/uwu_build_date_summary" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_version_name_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12.0dip"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_version_name"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_version_name" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_version_code"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_version_code_title"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_version_name_summary"
                        android:layout_alignStart="@+id/uwu_version_name_summary" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_version_code_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="12.0dip"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_version_code"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_version_code" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="17.0sp"
                        android:textColor="?android:textColorPrimary"
                        android:id="@+id/uwu_package_name"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/uwu_package_name_title"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_version_code_summary"
                        android:layout_alignStart="@+id/uwu_version_code_summary" />
                    <TextView
                        android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                        android:textSize="14.0sp"
                        android:textColor="?android:textColorSecondary"
                        android:ellipsize="marquee"
                        android:id="@+id/uwu_package_name_summary"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:clickable="true"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:layout_below="@+id/uwu_package_name"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:layout_alignStart="@+id/uwu_package_name" />
                </RelativeLayout>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/onClick"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0.0dip"
            android:layout_marginTop="4.0dip"
            android:layout_marginRight="0.0dip"
            android:layout_marginBottom="0.0dip"
            app:cardBackgroundColor="?colorCard"
            app:cardCornerRadius="5.0dip"
            app:cardElevation="0.0dip"
            app:rippleColor="?android:colorControlHighlight">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:clipToPadding="false"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16.0dip"
                        android:layout_marginBottom="16.0dip"
                        android:layout_marginStart="8.0dip"
                        android:layout_marginEnd="0.0dip">
                        <LinearLayout
                            android:background="@drawable/uwu_bg_icon"
                            android:layout_width="45.0dip"
                            android:layout_height="45.0dip"
                            android:layout_marginStart="8.0dip"
                            android:backgroundTint="?colorPrimary">
                            <androidx.preference.internal.PreferenceImageView
                                android:gravity="center"
                                android:id="@android:id/icon"
                                android:layout_width="25.0dip"
                                android:layout_height="25.0dip"
                                android:layout_marginTop="10.0dip"
                                android:tint="?android:textColorPrimaryInverse"
                                android:layout_marginStart="10.0dip"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </LinearLayout>
                    </LinearLayout>
                    <RelativeLayout
                        android:paddingTop="16.0dip"
                        android:paddingBottom="16.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:layout_marginStart="16.0dip"
                        android:layout_marginEnd="16.0dip">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                                android:textSize="17.0sp"
                                android:textColor="?android:textColorPrimary"
                                android:ellipsize="marquee"
                                android:id="@android:id/title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/uwu_banner_title"
                                android:singleLine="false" />
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@android:id/title"
                                android:layout_alignStart="@android:id/title">
                                <TextView
                                    android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
                                    android:textSize="14.0sp"
                                    android:textColor="?android:textColorSecondary"
                                    android:ellipsize="marquee"
                                    android:id="@android:id/summary"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:singleLine="false"
                                    android:marqueeRepeatLimit="marquee_forever" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16.0dip"
                        android:layout_marginBottom="16.0dip"
                        android:layout_marginStart="0.0dip"
                        android:layout_marginEnd="16.0dip">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="28.0dip"
                            android:layout_height="28.0dip"
                            app:cardBackgroundColor="?colorSurfaceContainerHighest"
                            app:cardCornerRadius="99.0dip"
                            app:cardElevation="0.0dip"
                            app:strokeWidth="0.0dip">
                            <ImageView
                                android:layout_width="25.0dip"
                                android:layout_height="25.0dip"
                                android:layout_marginTop="1.0dip"
                                android:src="@drawable/uwu_arrow"
                                android:tint="?colorOnSurfaceVariant"
                                android:layout_marginStart="2.0dip" />
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>
    </LinearLayout>
</com.google.android.material.card.MaterialCardView>