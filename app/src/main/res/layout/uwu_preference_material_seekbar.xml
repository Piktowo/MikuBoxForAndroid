<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:gravity="center_vertical"
    android:clipToPadding="false"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:minHeight="?android:listPreferredItemHeightSmall"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.neko.widget.LayoutOff
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginStart="8.0dip"
        android:layout_marginEnd="0.0dip"
        key="uwu_icon">
        <LinearLayout
            android:background="@drawable/uwu_bg_icon"
            android:layout_width="45.0dip"
            android:layout_height="45.0dip"
            android:layout_marginStart="8.0dip"
            android:backgroundTint="?colorPrimary">
            <androidx.preference.internal.PreferenceImageView
                android:gravity="center"
                android:id="@android:id/icon"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_marginTop="10.0dip"
                android:tint="?colorIcon"
                android:layout_marginStart="10.0dip"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </LinearLayout>
    </com.neko.widget.LayoutOff>
    <View
        android:layout_gravity="center_vertical"
        android:id="@android:id/empty"
        android:visibility="invisible"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip" />
    <RelativeLayout
        android:paddingTop="16.0dip"
        android:paddingBottom="16.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        android:layout_marginStart="16.0dip"
        android:layout_marginEnd="16.0dip">
        <Spinner
            android:id="@+id/spinner"
            android:visibility="invisible"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.0" />
        <TextView
            android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleMedium"
            android:textSize="17.0sp"
            android:textColor="?android:textColorPrimary"
            android:ellipsize="marquee"
            android:id="@android:id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="false" />
        <com.neko.widget.LayoutOff
            android:id="@+id/uwu_summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@android:id/title"
            android:layout_alignStart="@android:id/title"
            key="uwu_summary">
            <TextView
                android:textAppearance="@style/TextAppearance.M3.Sys.Typescale.TitleSmall"
                android:textSize="14.0sp"
                android:textColor="?android:textColorSecondary"
                android:ellipsize="marquee"
                android:id="@android:id/summary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="false"
                android:layout_below="@android:id/title"
                android:layout_alignStart="@android:id/title" />
        </com.neko.widget.LayoutOff>
        <androidx.preference.UnPressableLinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/seekbar_container"
            android:paddingLeft="0.0dip"
            android:paddingTop="8.0dip"
            android:paddingRight="16.0dip"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:layout_below="@+id/uwu_summary"
            android:paddingStart="0.0dip"
            android:paddingEnd="0.0dip"
            android:layout_alignStart="@+id/uwu_summary">
            <SeekBar
                android:id="@+id/seekbar"
                android:background="@null"
                android:paddingLeft="@dimen/preference_seekbar_padding_horizontal"
                android:paddingTop="0.0dip"
                android:paddingRight="@dimen/preference_seekbar_padding_horizontal"
                android:paddingBottom="0.0dip"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:progressDrawable="@drawable/uwu_progress_drawable"
                android:thumb="@null"
                android:layout_weight="1.0"
                android:paddingStart="0.0dip"
                android:paddingEnd="@dimen/preference_seekbar_padding_horizontal" />
            <TextView
                android:textAppearance="?android:textAppearanceListItem"
                android:textColor="?android:textColorSecondary"
                android:ellipsize="marquee"
                android:gravity="center"
                android:id="@+id/seekbar_value"
                android:paddingLeft="8.0dip"
                android:paddingRight="0.0dip"
                android:paddingBottom="0.0dip"
                android:scrollbars="none"
                android:fadingEdge="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/preference_seekbar_value_minWidth"
                android:singleLine="true"
                android:paddingStart="8.0dip"
                android:paddingEnd="0.0dip" />
        </androidx.preference.UnPressableLinearLayout>
    </RelativeLayout>
    <LinearLayout
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginStart="0.0dip"
        android:layout_marginEnd="8.0dip">
        <LinearLayout
            android:gravity="center"
            android:orientation="vertical"
            android:id="@android:id/widget_frame"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:paddingEnd="8.0dip" />
    </LinearLayout>
</LinearLayout>